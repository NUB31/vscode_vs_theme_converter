generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       String  @id @unique() @default(uuid())
  username String
  password String
  email    String  @unique(map: "email")
  picture  String?
  gender   String?
}
